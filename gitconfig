[color]
    ui = true
    diff = auto
    status = auto
    branch = auto
[color "status"]
    changed = yellow
    added = green
    untracked = red
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p
