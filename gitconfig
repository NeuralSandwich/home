[user]
	name = Sean Jones
	email = neuralsandwich@gmail.com
[core]
	autocrlf = input
	editor = vim
	excludesfile = /Users/neuralsandwich/.gitignore
[color]
    ui = true
    diff = auto
    status = auto
    branch = auto
[color "status"]
    changed = yellow
    added = green
    untracked = red
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --graph --date=short --all --pretty=format:'%C(blue)%h%C(reset) %C(green)%ad%C(reset) | %s %C(dim white)[%an]%C(reset)%C(yellow)%d%C(reset)'
  type = cat-file -t
  dump = cat-file -p
[push]
	default = current
